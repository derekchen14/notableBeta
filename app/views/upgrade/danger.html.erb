<%= render 'scaffold/header_out' %>
<div class="container">
  <%= render 'layouts/flash_msg' %>

  <h2>Notable Users</h1>

	<table class="table table-striped">
	<tr class="table-head">
    <td>Email</td>
    <td>ID</td>
    <td>Admin</td>
  </tr>
	<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= user.id %></td>
    <td><%= user.admin %></td>
    <td><%= link_to "delete", ultra_path(:id => user.id), method: :delete %></td>
  </tr>
  <% end %>
	</table>

	<%= link_to "Go back home", root_path %>


</div>