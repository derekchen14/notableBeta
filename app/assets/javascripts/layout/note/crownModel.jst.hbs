<div class="row">
	<div class="note-content col-sm-7 col-md-8" contenteditable="true">{{{title}}}</div>
	<!-- <div class="note-content">{{{subtitle}}}</div> -->
	<span class="feats hidden-xs col-sm-5 col-md-4">
		<span class="icon-icons-01" title="Tag"></span>
		<span class="icon-icons-07 glyphicon-copy" title="Copy"></span>
		<span class="icon-icons-11 glyphicon-export" title="Export"></span>
		<span class="icon-icons-04" title="Attach"></span>
		<span class="icon-icons-05" title="Emoticons"></span>
		<span class="icon-icons-06 destroy" title="Delete"></span>
	{{!--
		<span class="trash_icon sprite destroy"></span>
	  <span class="trash_icon glyphicon glyphicon-share"></span>
	  <span class="trash_icon glyphicon glyphicon-export"></span>
	 --}}
	</span>
</div>

